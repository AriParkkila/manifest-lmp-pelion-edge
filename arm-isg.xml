<manifest>
  <include name="default.xml" />

  <remote fetch="https://source.foundries.io/factories/arm-isg"
          name="subscriber-overrides" />

  <remote fetch="https://github.com/PelionIoT"
          name="PelionIoT" />

  <project name="meta-subscriber-overrides"
           path="layers/meta-subscriber-overrides"
           remote="subscriber-overrides"
           revision="master" />

  <project name="meta-mbed-edge"
           path="layers/meta-mbed-edge"
           remote="PelionIoT"
           revision="dev" />

  <project name="meta-pelion-edge"
           path="layers/meta-pelion-edge"
           remote="PelionIoT"
           revision="dev" />
</manifest>
